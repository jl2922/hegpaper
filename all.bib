@article{NeeRaj-SS-97,
Author = {Needs, RJ and Rajagopal, G},
Title = {{First-principles calculations of the adsorbate-induced surface stress of
   K/Al(111)-(root 3x root 3)R30 degrees}},
Journal = {{SURFACE SCIENCE}},
Year = {{1997}},
Volume = {{372}},
Number = {{1-3}},
Pages = {{179-184}},
Month = {{Feb 10}},
Abstract = {{We study the K/Al(111)-(root 3 x root 3)R30 degrees surface using a
   first-principles pseudopotential technique, presenting results for the
   on-top, fee and substitutional sites. In each case we calculate the
   surface energy and surface stress, which allows us to obtain the
   adsorption energy to linear order in an applied strain. These results
   are relevant to experiments involving the bending of a thin
   {[}111]-oriented Al crystal upon deposition of K atoms on one of its
   surfaces, and to the surface structure of K/Al(111) under an applied
   strain.}},
DOI = {{10.1016/S0039-6028(96)01134-X}},
