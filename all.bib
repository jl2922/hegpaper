journal = {J. Chem. Phys.},
number = {2},
pages = {024108},
pmid = {24437866},
title = {{Linear response theory for the density matrix renormalization group: efficient algorithms for strongly correlated excited states.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24437866},
volume = {140},
year = {2014}
}
