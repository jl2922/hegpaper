@article{MujNeeMun-PSSBR-96,
Author = {Mujica, A and Needs, RJ and Munoz, A},
Title = {{High pressure stability in III-V and II-VI binary compounds and the Cmcm
   phase - A theoretical study}},
Journal = {{Physica STATUS SOLIDI B-BASIC RESEARCH}},
Year = {{1996}},
Volume = {{198}},
Number = {{1}},
Pages = {{461-467}},
Month = {{Nov}},
Note = {{7th International Conference on High Pressure Semiconductor Physics
   (HPSP-VII), SCHWABISCH GMUND, GERMANY, Jul 28-31, 1996}},
Organization = {{Int Assoc Adv High Pressure Sci \& Technol}},
Abstract = {{We present the results of a theoretical study based on the density
   functional theory of the structural stability of several III-V and II-VI
   binary compounds (GaAs GaP, InAs, InP, ZnSe, and ZnTe, among others).
   The recent developments in X-ray diffraction techniques have led to the
   identification of several new phases and to a major change in the
   accepted trends for these families of compounds. A remarkable feature of
   the emerging new picture is the existence of an orthorhombic phase which
   is stable in many of the binary compounds studied so far, a distortion
   of the rocksalt structure which hay been named Cmcm after its space
   group. Our theoretical study focuses on this novel and rather general
   phase. We have investigated the instability of the rocksalt cell with
   respect to the distortion leading to Cmcm. The results of our ab initio
   study are in line with recent experimental studies of these compounds.}},
DOI = {{10.1002/pssb.2221980160}},
ISSN = {{0370-1972}},
ResearcherID-Numbers = {{Cavendish, TCM/C-9489-2009
   Needs, Richard/C-2931-2013}},
Unique-ID = {{ISI:A1996VX57300059}},
}

@article{KenRajNeeLeuGodWilFou-PRL-96,
Author = {Kenny, SD and Rajagopal, G and Needs, RJ and Leung, WK and Godfrey, MJ
   and Williamson, AJ and Foulkes, WMC},
Title = {{Quantum Monte Carlo calculations of the energy of the relativistic
   homogeneous electron gas}},
Journal = {{Phys. Rev. Lett.}},
Year = {{1996}},
Volume = {{77}},
Number = {{6}},
Pages = {{1099-1102}},
Month = {{Aug 5}},
Abstract = {{The ground state energy of the unpolarized homogeneous electron gas at
   zero temperature is calculated within the density range r(s) = 0.1-10,
   incorporating relativistic effects via first-order perturbation theory.
   Accurate nonrelativistic wave functions and variational and diffusion
   quantum Monte Carlo techniques are used to calculate the required
   expectation values.}},
DOI = {{10.1103/PhysRevLett.77.1099}},
ISSN = {{0031-9007}},
ResearcherID-Numbers = {{Cavendish, TCM/C-9489-2009
   Needs, Richard/C-2931-2013}},
Unique-ID = {{ISI:A1996VA47600030}},
}

@article{NeeRajWilKenFraFouJamMac-JKPS-96,
Author = {Needs, RJ and Rajagopal, G and Williamson, AJ and Kenny, SD and Fraser,
   LM and Foulkes, WMC and James, AJ and Maccallum, P},
Title = {{Quantum Monte Carlo studies of electronic systems}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{1996}},
Volume = {{29}},
Number = {{S}},
Pages = {{S116-S120}},
Month = {{Aug}},
Note = {{Pacific Conference on Condensed Matter Theory - Complex Materials and
   Strongly Correlated Systems, SEOUL, SOUTH KOREA, Dec 02-05, 1995}},
Organization = {{Korea Sci \& Engn Fdn; Korean Phys Soc}},
Abstract = {{We review our recent work on developing and applying quantum Monte Carlo
   (QMC) methods to electronic systems. The `'special k-points'' method is
   introduced into QMC calculations as a method for reducing `'independent
   particle'' finite size effects. We also explain our new formulation of
   the Coulomb interaction which virtually eliminates so-called Coulomb
   finite size effects. The importance of these advances for future
   applications of QMC techniques to electronic systems is stressed.}},
ISSN = {{0374-4884}},
ResearcherID-Numbers = {{Cavendish, TCM/C-9489-2009
   Needs, Richard/C-2931-2013}},
Unique-ID = {{ISI:A1996VC78700023}},
}

@article{McMRayNee-PBM-96,
Author = {McMullan, GJ and Ray, MP and Needs, RJ},
Title = {{Comparison of the calculated and observed Fermi surfaces of Sr2RuO4}},
Journal = {{Physica B-CONDENSED MATTER}},
Year = {{1996}},
Volume = {{223-24}},
Number = {{1-4}},
Pages = {{529-531}},
Month = {{Jun}},
Abstract = {{We compare the normal state Fermi surface of Sr(2)RuO(4) as inferred
   from a recent magneto-oscillatory study with the predictions of local
   spin density electronic band structure calculations and find good
   agreement between the observed and calculated topologies. Identification
   of the observed quasi-particle orbits with those predicted shows that
   the quasiparticle masses are enhanced by a factor of three; which is
   consistent with the enhancement of the observed specific heat.}},
DOI = {{10.1016/0921-4526(96)00164-0}},
ISSN = {{0921-4526}},
ResearcherID-Numbers = {{Needs, Richard/C-2931-2013}},
Unique-ID = {{ISI:A1996UX82200140}},
}

@article{WilKenRajJamNeeFraFouMac-PRB-96,
Author = {Williamson, AJ and Kenny, SD and Rajagopal, G and James, AJ and Needs,
   RJ and Fraser, LM and Foulkes, WMC and Maccullum, P},
Title = {{Optimized wave functions for quantum Monte Carlo studies of atoms and
   solids}},
Journal = {{Phys. Rev. B}},
Year = {{1996}},
Volume = {{53}},
Number = {{15}},
Pages = {{9640-9648}},
Month = {{Apr 15}},
Abstract = {{Wave functions for the homogeneous electron gas, a germanium
   pseudosolid, and a germanium pseudoatom are optimized using the method
   of variance minimization. Forms for the Jastrow factor which are
   convenient to optimize and may be evaluated rapidly are devised and
   tested and we stress the advantages of using expressions which are
   linear in the variable parameters. For each system studied we have
   performed variational and diffusion quantum Monte Carlo calculations to
   test the accuracy of the optimized wave functions. The results of our
   study are very promising for future applications of quantum Monte Carlo
   methods to real materials.}},
DOI = {{10.1103/PhysRevB.53.9640}},
ISSN = {{1098-0121}},
ResearcherID-Numbers = {{Cavendish, TCM/C-9489-2009
   Needs, Richard/C-2931-2013}},
Unique-ID = {{ISI:A1996UJ29600037}},
}

@article{MujNee-JPM-96,
Author = {Mujica, A and Needs, RJ},
Title = {{The Cmcm structure as a stable phase of binary compounds: Application to
   GaAs-II}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{1996}},
Volume = {{8}},
Number = {{15}},
Pages = {{L237-L243}},
Month = {{Apr 8}},
Abstract = {{We have studied the orthorhombic high-pressure phase II of GaAs. We find
   that the structure previously suggested for this phase is unstable and
   leads to the same Cmcm structure which was recently found in
   high-pressure studies of ZnTe and CdTe. We have investigated the
   mechanism for the distortion leading to Cmcm. Our calculations show that
   Cmcm GaAs becomes thermodynamically stable at 120 kbar in agreement with
   experimental data for GaAs-II. The available experimental structural
   parameters are consistent with this Cmcm structure, which may be common
   among II-VI and III-V compounds.}},
DOI = {{10.1088/0953-8984/8/15/001}},
ISSN = {{0953-8984}},
ResearcherID-Numbers = {{Cavendish, TCM/C-9489-2009
   Needs, Richard/C-2931-2013}},
Unique-ID = {{ISI:A1996UG22300001}},
}
