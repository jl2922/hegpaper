@article{GruCepLal-PRL-97,
Author = {Gruter, P and Ceperley, D and Laloe, F},
Title = {{Critical temperature of Bose-Einstein condensation of hard-sphere gases}},
Journal = {{Phys. Rev. Lett.}},
Year = {{1997}},
