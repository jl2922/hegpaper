@article{HOHNATCEPMAR-PRL-93,
Author = {HOHL, D and NATOLI, V and CEPERLEY, DM and MARTIN, RM},
Title = {{MOLECULAR-DYNAMICS IN DENSE HYDROGEN}},
Journal = {{Phys. Rev. Lett.}},
Year = {{1993}},
Volume = {{71}},
Number = {{4}},
Pages = {{541-544}},
Month = {{Jul 26}},
Abstract = {{We present ab initio molecular dynamics simulations for high-density
   hydrogen r(s)=1.78-1.31, P almost-equal-to 35-150 GPa). Protons are
   treated as classical point charges and electrons in local density
   functional theory. The basic structural entity in this density range is
   still the hydrogen molecule but applying pressure changes the molecular
   ordering drastically. Molecules line up to form filaments and eventually
   planes, where intermolecular distances become comparable to the H-2 bond
   length. The phase diagram is strongly temperature dependent, and many
   different structures with very similar stability exist.}},
DOI = {{10.1103/PhysRevLett.71.541}},
ISSN = {{0031-9007}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008}},
Unique-ID = {{ISI:A1993LP04500018}},
