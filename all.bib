@article{KenHooTowNeeRaj-PRB-98,
Author = {Kent, PRC and Hood, RQ and Towler, MD and Needs, RJ and Rajagopal, G},
Title = {{Quantum Monte Carlo calculations of the one-body density matrix and
   excitation energies of silicon}},
Journal = {{Phys. Rev. B}},
Year = {{1998}},
Volume = {{57}},
Number = {{24}},
Pages = {{15293-15302}},
Month = {{Jun 15}},
Abstract = {{Quantum Monte Carlo (QMC) techniques are used to calculate the one-body
   density matrix and excitation energies for the valence electrons of bulk
   silicon. The one-body density matrix and energies are obtained from a
   Slater-Jastrow wave function with a determinant of local-density
   approximation (LDA) orbitals. The QMC density matrix evaluated in a
   basis of LDA orbitals is strongly diagonally dominant. The natural
   orbitals obtained by diagonalizing the QMC density matrix resemble the
   LDA orbitals very closely. Replacing the determinant of LDA orbitals in
   the wave function by a determinant of natural orbitals makes no
   significant difference to the quality of the wave function's nodal
   surface, leaving the diffusion Monte Carlo energy unchanged. The
   extended Koopmans's theorem for correlated wave functions is used to
   calculate excitation energies for silicon, which are in reasonable
   agreement with the available experimental data. A diagonal approximation
   to the theorem, evaluated in the basis of LDA orbitals, works quite well
   for both the quasihole and quasielectron states. We have found that this
   approximation has an advantageous scaling with system size, allowing
   more efficient studies of larger systems.}},
DOI = {{10.1103/PhysRevB.57.15293}},
ISSN = {{1098-0121}},
ResearcherID-Numbers = {{Kent, Paul/A-6756-2008
   Cavendish, TCM/C-9489-2009
   Needs, Richard/C-2931-2013}},
