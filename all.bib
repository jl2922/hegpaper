@article{MAGCEP-PRL-94,
Author = {MAGRO, WR and CEPERLEY, DM},
Title = {{GROUND-STATE PROPERTIES OF THE 2-DIMENSIONAL BOSE COULOMB LIQUID}},
Journal = {{Phys. Rev. Lett.}},
Year = {{1994}},
Volume = {{73}},
Number = {{6}},
Pages = {{826-829}},
Month = {{Aug 8}},
Abstract = {{We present results of a ground-state study, via the diffusion Monte
   Carlo method, of a two-dimensional charged boson liquid. We observe
   three distinct behaviors in this system. At the lowest densities a
   close-packed Wigner crystal forms. At r(s) almost-equal-to 12 the
   crystal melts into a noncondensed liquid exhibiting algebraic
   off-diagonal long-range order; the single-particle density matrix decays
   as r(-r(s)/4). Above a threshold density (r(s) < 8), the long-wavelength
   momentum distribution diverges as k(r(s)-8)/4, although no condensate
   forms.}},
DOI = {{10.1103/PhysRevLett.73.826}},
ISSN = {{0031-9007}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008}},
Unique-ID = {{ISI:A1994PA37100013}},
}

@article{KWOCEPMAR-PRB-94,
Author = {KWON, YK and CEPERLEY, DM and MARTIN, RM},
Title = {{QUANTUM MONTE-CARLO CALCULATION OF THE FERMI-LIQUID PARAMETERS IN THE
   2-DIMENSIONAL ELECTRON-GAS}},
Journal = {{Phys. Rev. B}},
Year = {{1994}},
Volume = {{50}},
Number = {{3}},
Pages = {{1684-1694}},
Month = {{Jul 15}},
Abstract = {{Excitations of the two-dimensional electron gas, including many-body
   effects, are calculated with a variational Monte Carlo method.
   Correlated sampling is introduced to calculate small energy differences
   between different excitations. The usual pair-product (Slater-Jastrow)
   trial wave function is found to lack certain correlations entirely so
   that backflow correlation is crucial. From the excitation energies
   calculated here, we determine Fermi-liquid parameters and related
   physical quantities such as the effective mass and the Lande g factor of
   the system. Our results for the effective mass are compared with
   previous analytic calculations.}},
DOI = {{10.1103/PhysRevB.50.1684}},
ISSN = {{0163-1829}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008}},
Unique-ID = {{ISI:A1994NZ15300039}},
}

@article{ACICEP-JCP-94,
Author = {ACIOLI, PH and CEPERLEY, DM},
Title = {{GENERATION OF PSEUDOPOTENTIALS FROM CORRELATED WAVE-FUNCTIONS}},
Journal = {{J. Chem. Phys.}},
Year = {{1994}},
Volume = {{100}},
Number = {{11}},
Pages = {{8169-8177}},
Month = {{Jun 1}},
Abstract = {{The density matrix, or equivalently the natural orbitals play an
   essential role in determining the transferability of pseudopotentials to
   all orders of perturbation theory. In this work the one-particle density
   matrix and natural. orbitals of Li, C, and Ne atoms are obtained using
   variational and diffusion Monte Carlo. Using these a pseudopotential is
   computed for the lithium atom.}},
