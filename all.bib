@article{MilMagCep-CTPP-99,
Author = {Militzer, B and Magro, W and Ceperley, D},
Title = {{Characterization of the state of hydrogen at high temperature and
   density}},
Journal = {{CONTRIBUTIONS TO PLASMA PHYSICS}},
Year = {{1999}},
Volume = {{39}},
Number = {{1-2}},
Pages = {{151-154}},
Note = {{9th International Workshop on the Physics of Nonideal Plasmas (PNP-9),
