@article{AciCep-PRB-96,
Author = {Acioli, PH and Ceperley, DM},
Title = {{Diffusion Monte Carlo study of jellium surfaces: Electronic densities
   and pair correlation functions}},
Journal = {{Phys. Rev. B}},
Year = {{1996}},
Volume = {{54}},
Number = {{23}},
Pages = {{17199}},
Month = {{Dec 15}},
Abstract = {{We present the results of fixed-node diffusion Monte Carlo calculations
   of jellium surfaces for metallic densities. We used a trial wave
   function of the Slater-Jastrow type, with the long-range part of the
   two-body term modified to account for the anisotropy of the system. The
   one-body term is optimized so that the electronic density from
   variational and diffusion Monte Carlo calculations agree with each
   other. The final densities are close to the densities obtained from
   density-functional calculations in the local-density approximation and
   the surface energies to the results obtained using the Langreth-Mehl and
   Perdew-Wang generalized gradient approximation at high densities (r(s)
   less than or equal to 2.07). At low densities (r(s) greater than or
   equal to 3.25) they agree with the results of the
   Fermi-hypernetted-chain calculations. The pair-correlation functions at
   regions near the surface are tabulated, showing the anisotropy of the
   exchange-correlation hole in regions of fast-varying densities.}},
DOI = {{10.1103/PhysRevB.54.17199}},
ISSN = {{0163-1829}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008}},
Unique-ID = {{ISI:A1996WA83300127}},
}

@article{SenMorCep-JNS-96,
Author = {Senatore, G and Moroni, S and Ceperley, DM},
Title = {{Local field factor and effective potentials in liquid metals}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{1996}},
Volume = {{207}},
Number = {{2}},
Pages = {{851-854}},
Month = {{Oct}},
Note = {{9th International Conference on Liquid and Amorphous Metals, CHICAGO,
   IL, Aug 27-Sep 01, 1995}},
Abstract = {{The authors recently evaluated the local field factor, G(q) of the
   electron gas by accurate fixed-node diffusion Monte Carlo (DMC)
   simulations. Here, the G(q) obtained from a simple fit to the DMC
   results and from several approximations available in the literature is
   used to derive the effective pair potential phi(r) in simple metals
   within second-order perturbation theory. Using a local electron-ion
   pseudopotential, it is found that phi is determined mostly by the
   behavior of G(q) at q less than or equal to ca. 2q(F), where the DMC
   data are well reproduced by the local field within local density
   approximation, G(LDA)) = A(q/q(F))(2) with A related to the
   compressibility of the uniform electron gas.}},
ISSN = {{0022-3093}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008}},
Unique-ID = {{ISI:A1996WB29300080}},
}

@article{TimEvaBonCepMaySim-JPM-96,
Author = {Timms, DN and Evans, AC and Boninsegni, M and Ceperley, DM and Mayers, J
   and Simmons, RO},
Title = {{Direct measurements and path integral Monte Carlo calculations of
   kinetic energies of solid neon}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{1996}},
Volume = {{8}},
Number = {{36}},
Pages = {{6665-6684}},
Month = {{Sep 2}},
Abstract = {{Direct measurements are reported of the kinetic energies (E(k)) of FCC
   neon at seven temperatures between 4.25 and 20.2 K. The measurements
   employed the electronvolt spectrometer which is the neutron scattering
   instrument at the ISIS neutron source, UK. Wavevector transfers Q from
   306 to 1158 nm(-1) were used, far larger than previously, in a regime
   where possible final-state-effect Corrections to the observed
   longitudinal neutron Compton profile J(y) are easily deal with by a
   simple correctional procedure. The results are of comparable precision
   with the best previous work carried out using the HRMECS instrument at
   IPNS, Argonne. From those previous measurements, Peek et al give a
   ground-state kinetic energy of 49.1 +/- 2.8 K. Path-integral Monte Carlo
   calculations were performed using both HFD-C2 and Lennard-Jones pair
   potentials. At 4.13 K, 10.13 K, 15.69 K and 20.31 K the HFD results were
   41.6 +/- 0.1 K, 42.6 +/- 0.1 K, 46.5 +/- 0.1 K and 47.8 +/- 0.1 K,
   respectively. These values are broadly in agreement with the current
   measurements. They are significantly lower than the published values
   obtained from an HFD-based Wigner-Kirkwood high-temperature expansion
   including terms up to <(h)over bar(6)>.}},
DOI = {{10.1088/0953-8984/8/36/018}},
ISSN = {{0953-8984}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008
   Boninsegni, Massimo/G-3367-2011}},
Unique-ID = {{ISI:A1996VF31700018}},
}

@article{BonCep-JLTP-96,
Author = {Boninsegni, M and Ceperley, DM},
Title = {{Density fluctuations in liquid He-4. Path integrals and maximum entropy}},
Journal = {{JOURNAL OF LOW TEMPERATURE PHYSICS}},
Year = {{1996}},
Volume = {{104}},
Number = {{5-6}},
Pages = {{339-357}},
Month = {{Sep}},
Abstract = {{We estimate the dynamic structure factor S(q, omega) for liquid He-4 in
   both ifs normal and superfluid phases. A path integral Monte Carlo
   simulation is performed to compute the imaginary-time polarization
   propagator F(q, tau), from which S(q, omega) is extracted by maximum
   entropy. Results for normal He-4 ave in good quantitative agreement with
   recent neutron scattering experimental data; broad agreement is found
   for superfluid 4He as well, though sharp features are lost, particularly
   at low q. We attribute the excessive smoothness of the results to the
   entropic `'prior probability'' function used in the maximum entropy
   reconstruction. The experimentally observed ground state excitation
   spectrum E(q) is accurately reproduced in the 0 less than or equal to q
   less than or equal to 2.5 Angstrom(-1) range.}},
DOI = {{10.1007/BF00751861}},
ISSN = {{0022-2291}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008
   Boninsegni, Massimo/G-3367-2011}},
Unique-ID = {{ISI:A1996VK39000003}},
}

@article{JonOrtCep-PRA-96,
Author = {Jones, MD and Ortiz, G and Ceperley, DM},
Title = {{Hartree-Fock studies of atoms in strong magnetic fields}},
Journal = {{Phys. Rev. A}},
Year = {{1996}},
Volume = {{54}},
Number = {{1}},
Pages = {{219}},
Abstract = {{We present comprehensive calculations of the electronic structure of
   selected first-row atoms in uniform magnetic fields of strength less
   than or equal to 10(10) G, within a flexible implementation of the
   Hartree-Fock formalism. Groundstate and low-lying excited state
   properties are presented for first-row atoms He, Li, C, and ion H-. We
   predict and describe a series of ground-state quantum transitions as a
   function of magnetic field strength. Due to its astrophysical
   importance, highly excited states of neutral He are also computed.
   Comparisons are made with previous works, where available.}},
DOI = {{10.1103/PhysRevA.54.219}},
ISSN = {{1050-2947}},
ResearcherID-Numbers = {{Ceperley, David/A-6858-2008}},
Unique-ID = {{ISI:A1996UX03400027}},
